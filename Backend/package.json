{
    "name": "demo-api",
    "version": "1.0.0",
    "description": "Demo API",
    "scripts": {
        "inst": "npm install",
        "repair": "rimraf dist node_modules package-lock.json && npm run inst",
        "dev": "nodemon",
        "build:tsc": "tsc && tsc-alias",
        "build": "swc app -d dist --source-maps --copy-files &&  npm run copy-static-assets",
        "local": "swc app -d dist --source-maps --copy-files --sync --watch && tsc --pretty --skipLibCheck --noEmit & node dist/server.js",
        "start": "node server.js",
        "lint": "eslint ./app  --ext .ts",
        "lint:fix": "eslint ./app --ext .ts --fix",
        "style:check": "prettier --check app/*.ts app/**/*.ts",
        "style:fix": "prettier --write app/*.ts app/**/*.ts",
        "fix": "npm run lint:fix && npm run style:fix",
        "test": "npm run lint && npm run style:check && npm run test:unit && npm run test:functional",
        "prepare": "husky install"
    },
    "keywords": [
        "Node",
        "openapi",
        "Swagger",
        "Prettier",
        "Eslint",
        "API",
        "Auth0",
        "MongoDb",
        "Typescript",
        "SWC: Speedy Web Compiler"
    ],
    "author": "YOUR_NAME",
    "dependencies": {
        "axios": "^0.24.0",
        "compression": "^1.7.4",
        "config": "^3.3.6",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "envalid": "^7.2.2",
        "express": "^4.17.2",
        "express-validator": "^6.14.0",
        "helmet": "^5.0.1",
        "hpp": "^0.2.3",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "moment": "^2.29.3",
        "moment-timezone": "^0.5.34",
        "mongoose": "^6.1.5",
        "morgan": "^1.10.0",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.3.0",
        "uuid": "^8.3.2",
        "winston": "^3.3.4"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.55",
        "@swc/core": "^1.2.131",
        "@types/async": "^3.2.12",
        "@types/compression": "^1.7.2",
        "@types/config": "0.0.41",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/helmet": "^4.0.0",
        "@types/hpp": "^0.2.2",
        "@types/lodash": "^4.14.178",
        "@types/mongoose": "^5.11.97",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.8",
        "@types/nodemailer": "^6.4.4",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^12.1.7",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.2",
        "ts-node": "^10.4.0",
        "tsc-alias": "^1.5.0",
        "tsc-watch": "^4.6.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.4"
    }
}
